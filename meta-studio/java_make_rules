ANT_ENV = \
  BUILDFILE=${top_srcdir}/build.xml \
  JAR=${JARFILE} \
  MAIN_CLASS=${MAIN_CLASS} \
  LOCAL_JARS=${LOCAL_JARS} \
  EXTERNAL_JARS=${EXTERNAL_JARS}

if MAINTAINER_MODE
${JARFILE}: dummy
	java -version
	java -target 1.5
	java -target 1.6
	${ANT_ENV} ant dist

dummy:

clean-local:
	${ANT_ENV} ant clean
endif
