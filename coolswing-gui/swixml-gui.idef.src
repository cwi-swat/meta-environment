tool swixml is { }

process SWIXML is
let
  T : swixml,
  Message: str,
  Path: str,
  File: str
in
  rec-connect(T?)
  .
  (
    rec-msg(swixml-display(Path?, File?))
    . snd-do(T, swixml-display(Path, File))
  )
  * delta
endlet

process StartSWIXML is
  snd-msg(load-jar("file://__PKGDATA__/__JARFILE__", "__EXTERNAL_JARS__:__LOCAL_JARS__"))

toolbus(SWIXML)
