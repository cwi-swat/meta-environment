# @configure_input@
# Directory: new-meta

# Configure variable substitutions
PRODUCT		= @PRODUCT@
VERSION		= @VERSION@

@SET_MAKE@

srcdir		= @srcdir@
VPATH		= @srcdir@
top_srcdir	= @top_srcdir@
prefix		= @prefix@
exec_prefix	= @exec_prefix@
bindir		= ${exec_prefix}/bin
libdir		= ${exec_prefix}/lib
includedir	= ${prefix}/include
classdir        = ${prefix}/classes/nl/cwi/gipe/stred

SHELL		= @SHELL@

INSTALL		= @INSTALL@
INSTALL_DATA	= @INSTALL_DATA@
INSTALL_PROG	= @INSTALL_PROG@
UNINSTALL	= @UNINSTALL@

# Ordering is important here: the toolbus dir. should come first
SUBDIRS		= toolbus editor evaluator graph-browser import-db \
		in-output tree-db compiler stred asource a2toa1

all doc clean:
	@for DIR in $(SUBDIRS);	\
	do \
		echo Make $@ in $$DIR; \
		cd $$DIR; \
		$(MAKE) $@; \
		cd ..; \
	done

uninstall:
	$(UNINSTALL) $(bindir) meta
	@for DIR in $(SUBDIRS);	\
	do \
		echo Make $@ in $$DIR; \
		cd $$DIR; \
		$(MAKE) $@; \
		cd ..; \
	done

install:
	mkinstalldirs $(bindir) $(libdir) $(includedir) $(classdir)
	$(INSTALL_PROG) $(INSTALL) $(bindir) meta
	@for DIR in $(SUBDIRS);	\
	do \
		echo Make $@ in $$DIR; \
		cd $$DIR; \
		$(MAKE) $@; \
		cd ..; \
	done

distclean:
	rm -f Makefile Makefile.skel config.cache config.log config.status
	rm -f install-data-sh install-prog-sh meta
	@for DIR in $(SUBDIRS);	\
	do \
		echo Make $@ in $$DIR; \
		cd $$DIR; \
		$(MAKE) $@; \
		cd ..; \
	done

depend:



