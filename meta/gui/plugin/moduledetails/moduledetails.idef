#ifndef __MODULE_DETAILS__
#define __MODULE_DETAILS__

tool moduledetails is { }

process Moduledetails is
let
  Details : term,
  T: moduledetails
in
  rec-connect(T?)
  .
    (
      rec-msg(md-set-details(Details?))
      . snd-do(T, set-details(Details))
    )
    *
      rec-disconnect(T)
endlet

toolbus(Moduledetails)

#endif /*__MODULE_DETAILS__*/
