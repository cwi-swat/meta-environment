module EditorManager

imports basic/NatCon
imports basic/StrCon
imports basic/Whitespace

exports
  sorts Sid
  context-free syntax
    "sid" "(" id:NatCon ")" -> Sid

  sorts EditorType
  context-free syntax
    name:StrCon -> EditorType

  sorts Session
  context-free syntax
    "session-data" "("
      id:Sid ","
      filename:StrCon ","
      modulename:StrCon ","
      "{" {EditorType ","}* "}"
    ")"
    -> Session
