# $Id$

# Configure variable substitutions
PRODUCT		= @PRODUCT@
VERSION		= @VERSION@

@SET_MAKE@

srcdir		= @srcdir@
VPATH		= @srcdir@
top_srcdir	= @top_srcdir@
prefix		= @prefix@
exec_prefix	= @exec_prefix@
bindir		= ${exec_prefix}/bin
libdir		= ${exec_prefix}/lib
includedir	= ${prefix}/include

SHELL		= @SHELL@
INSTALL		= @INSTALL@
INSTALL_DATA	= @INSTALL_DATA@
INSTALL_PROG	= @INSTALL_PROG@
UNINSTALL	= @UNINSTALL@

# Ordering is important here: the toolbus dir. should come first
UTILS		= gen-table parse-sdf2 sdf2table

all doc clean:	${UTILS}

uninstall:
	$(UNINSTALL) $(bindir) ${UTILS}

install:	all
	$(INSTALL_PROG) $(INSTALL) $(bindir) ${UTILS}

distclean:

depend:



