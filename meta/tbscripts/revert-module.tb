/*
  $Id$
 */
process RevertModule is
let 
  Id         : term,
  ModuleName : str
in 
  Id := process-id. 
  (
    rec-msg(revert-module(ModuleName?)) .
    snd-note(ui-status(statf(Id,"Reverting %s",[ModuleName]))) .
    snd-msg(close-module(ModuleName)) .
    rec-msg(module-closed(ModuleName)) .
    snd-msg(open-modules([ModuleName])) .
    rec-msg(finished-opening-modules) .
    snd-note(ui-status(endstat(Id))) .  
    snd-msg(module-reverted(ModuleName))
  ) *
  delta
endlet
