#include "meta.tb"
#include "status.tb"

process StandalonePtDumper is
let
  ModuleName : str,
  Pid : int
in
  create(StatusDisplay, Pid?) .
  create(MetaCreator, Pid?) .
  rec-msg(creation-finished) .
  snd-msg(open-modules([MODULENAME], sdf)) .
  rec-msg(finished-opening-modules) .
  snd-msg(dump-parse-table-given-file(MODULENAME, OUTFILENAME)) .
  rec-msg(parse-table-dumped) .
  shutdown("")

endlet   

toolbus(StandalonePtDumper)
