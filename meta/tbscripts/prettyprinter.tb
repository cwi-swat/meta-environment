/* $Id$ */

tool asource is {
  command = "/home/markvdb/NEW-META/bin/asource"
}

process Prettyprinter is
let
  PP: asource,
  T: term,
  Str: str,
  Src: term,
  Clst: list,
  Flst: list,
  Tslst: list
in
  execute(asource,PP?).
  (  rec-msg(pp-module(T?)).
    snd-eval(PP,conssource(T)).
    rec-value(PP,source(Src?)).
    snd-msg(module-source(Src))
  ) *
  delta
endlet
