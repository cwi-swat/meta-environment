tool asource is {
  command = "asource"
}

process Prettyprinter is
let
  PP   : asource,
  Tree : term, 
  Text : term 
in
  execute(asource,PP?).
  ( rec-msg(unparse(Tree?)).
    snd-eval(PP,conssource(Tree)).
    rec-value(PP,source(Text?)).
    snd-msg(unparsed-text(Text)) 
  ) *
  delta
endlet
