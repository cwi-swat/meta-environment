 */

process Clear-all(Eval: evaluator, Tdb: module-db) is
let
Id: term
in
  Id := process-id. (
    rec-msg(clear-all).
    snd-note(ui-status(stat(Id,"Clearing ..."))).
    snd-do(Tdb, clear-module-db).
/*    snd-do(Eval, clear-equations-db).*/
    snd-note(ui-status(endstat(Id))).
    snd-msg(cleared-all)
  ) *
  delta
endlet
