#include "../../in-output/in-output.idef"
#include "../../module-db/module-db.idef"
#include "../file-creator.tb"

process Test is
  snd-msg(create-new-module("ExistingModule")) .
  (
    rec-msg(module-created) .
    printf("new-module-test-2 FAILED!\n")
  +
    rec-msg(module-not-created("Module already exists in text or baf"))
  +
    shutdown("new-module-test-2 FAILED! (timeout)\n") delay(sec(5))
  ) .
  shutdown("") delay(sec(2))

toolbus(Test, Io, ModuleDB, File-creator)
