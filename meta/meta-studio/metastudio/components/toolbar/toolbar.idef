tool tool-bar is { }

process ToolBar is
let
  EventList: list,
  Event: term,
  Type: term,
  T: tool-bar
in
  rec-connect(T?) 
  . snd-msg(cm-get-events(studio-toolbar))
  . rec-msg(cm-events(EventList?))
  . snd-do(T, add-events(EventList))
  .
    (
      rec-event(T, button-selected(Event?))
      . snd-msg(ui-button-selected(studio-toolbar, Event))
      . snd-ack-event(T, button-selected(Event))
    )
    *
      rec-disconnect(T)
endlet

toolbus(ToolBar)
