#include "in-output.idef"

process Test is
  snd-msg(exists-sdf2-module("NonExistingModule")) .
  (
    rec-msg(sdf2-module-not-exists("NonExistingModule"))
  +
    rec-msg(sdf2-module-exists("NonExistingModule")) .
    printf("in-output-test-1 FAILED!\n")
  +
    shutdown("test-1 FAILED! (timeout)\n") delay(sec(5))
  ) .
  shutdown("") delay(sec(2))

toolbus(Test, Io)
