#include <in-output.idef>

process Test is
  snd-msg(process-search-paths("meta.conf", ["__SRCDIR__"])) .
  rec-msg(search-paths-processed("meta.conf")) .
  snd-msg(exists-sdf-module("Empty2")) .
  (
    rec-msg(sdf-module-exists("Empty2")) .
    printf("in-output-test-3 FAILED!\n")
  +
    rec-msg(sdf-module-not-exists("Empty2"))
  +
    shutdown("test-3 FAILED! (timeout)\n") delay(sec(5))
  ) .
  shutdown("") delay(sec(2))

toolbus(Test, Io)
