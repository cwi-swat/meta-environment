#include "../in-output.idef"

process Test is
  snd-msg(exists-sdf2-module("Empty2")) .
  (
    rec-msg(sdf2-module-exists("Empty2"))
  +
    rec-msg(sdf2-module-not-exists("Empty2")) .
    printf("in-output-test-3 FAILED!\n")
  +
    shutdown("test-3 FAILED! (timeout)\n") delay(sec(5))
  ) .
  shutdown("") delay(sec(2))

toolbus(Test, Io)
