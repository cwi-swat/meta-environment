#include <in-output.idef>

process Test is
let
  EqsText : str,
  EqsTree : term,
  Path    : str,
  Time    : term,
  Msg     : str
in
  snd-msg(open-asf-file("TestEqs", "__SRCDIR__/TestEqs.sdf")) .
  (
    rec-msg(error-opening("TestEqs"))
  +
    (
      rec-msg(opened-file("TestEqs", tree(EqsTree?), Path?, Time?))
    +
      rec-msg(opened-file("TestEqs", text(EqsText?), Path?, Time?))
    ) .
    printf("in-output-test-7 FAILED!\n")
  +
    shutdown("test-7 FAILED! (timeout)\n") delay(sec(5))
  ) .
  shutdown("") delay(sec(2))
endlet

toolbus(Test, Io)
