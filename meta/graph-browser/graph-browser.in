#! @SHELL@
# $Id$

usage () {
  echo "usage: $1 [toolbus-options]"
  echo "note that this program can ONLY be used as a ToolBus tool!"
  exit 1
}

version () {
  echo "graph-browser v0.1"
  exit 1
}

if [ $1 = "-h" ]
then
  usage $0
fi

if [ $1 = "-V" -o $1 = "--version" ]
then
  version $0
fi

prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
datadir=@datadir@
TCLLIBPATH=@TCLDOT@; export TCLLIBPATH
WISH=@WISH@
wish-adapter -wish $WISH -script $datadir/graph-browser.tcl $@
