tool svg is { }

process SVG is
let
  T : svg,
  Title: str,
  SVG: str
in
  rec-connect(T?)
  .
  (
    rec-msg(svg-display(Title?,SVG?))
    . snd-do(T, display(Title,SVG))
  )
  * delta
endlet

process StartSVG is
  snd-msg(load-jar("file://__PKGDATA__/__JARFILE__", "__EXTERNAL_JARS__"))

toolbus(SVG)
