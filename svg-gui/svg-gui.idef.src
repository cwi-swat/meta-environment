#ifndef SVG_GUI
#define SVG_GUI

tool svg is { }

process SVG is
let
  T: svg,
  Title: str,
  File: str
in
  rec-connect(T?)
  . snd-do(T, display("test", "/home/jurgenv/example.svg"))
  . 
  (
    rec-msg(svg-display(Title?, File?))
    . snd-do(T, display(Title, File))
  )
  * delta
endlet

toolbus(SVG)

process StartSVG is
  snd-msg(load-jar("file://__PKGDATA__/__JARFILE__", "__EXTERNAL_JARS__"))


#endif
