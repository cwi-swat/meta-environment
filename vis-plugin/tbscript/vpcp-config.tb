   snd-msg(load-jar("file:///home/jurgenv/glt/svn-installed/vis-plugin/share/vis-plugin.jar", "::/home/jurgenv/glt/svn-installed/aterm-java/share/aterm-java.jar:/home/jurgenv/glt/svn-installed/shared-objects/share/shared-objects.jar:/home/jurgenv/glt/svn-installed/JJTraveler/share/jjtraveler.jar:/home/jurgenv/glt/svn-installed/config-support/share/configapi.jar:/home/jurgenv/glt/svn-installed/toolbus-java-adapter/share/toolbus-java-adapter.jar:/home/jurgenv/glt/svn-installed/meta-studio/share/meta-studio.jar:/home/jurgenv/glt/svn-installed/meta-studio/share/idw-gpl-1.5.0.jar:/home/jurgenv/glt/svn-installed/svg-support/share/svg-support/batik.jar:/home/jurgenv/glt/svn-installed/tunit/share/tunit.jar:/home/jurgenv/glt/svn-installed/tunit/share/tunit/junit.jar"))

