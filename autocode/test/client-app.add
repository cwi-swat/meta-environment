application client-app {
  generator java {
    enabled := true

    @import "Client.add"
    @import "Order.add"

    type * {
      access := private
    }

    type Client {
      access := public
      operation := { constructor }
      interface := { Serializable, Runnable }
  
      field name {
        operation += { init }
        access := public
        static := true
      }
      
      field * {
        operation := { get, set }
      }

      operation constructor {
        access := proctected
      }
    }
  }
}

