application client-app {
  generator java {
    enabled := true

    @import "Client.add"
    @import "Order.add"

    type * {
      access := private
      package := "org.autocode.test"
    }

    type Client {
      access := public
      operation := { constructor }
      interface := { Serializable, Runnable }
  
      field name {
        type string {
          implementation := "MyString"
          implementation "MyString" {
            import := "nl.cwi.aterm"
          }
        }
        operation += { init }
        access := public
        static := true
      }

      field order {
        implementation list {
          type := "ArrayList"
          type "ArrayList" { import := "java.util.ArrayList" }
        }
      }
      
      field * {
        operation := { get, set }
      }

      operation constructor {
        access := proctected
      }
    }
  }
}

