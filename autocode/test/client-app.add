application client-app {
  generator java {
    enabled := true
    javadoc := true

    @import "Client.add"
    @import "Order.add"

    type * {
      package := "org.autocode.test"
    }

    type Client {
      operation := { constructor }
      interface := { Serializable, Runnable }
  
      field * { access := public }

      field name {
        operation += { init }
        access := public
        static := true
      }

      field order {
        implementation list {
          type := "ArrayList"
          type "ArrayList" { import := "java.util.ArrayList" }
        }
      }
      
      field * {
        operation := { get, set }
      }
    }
  }
}

