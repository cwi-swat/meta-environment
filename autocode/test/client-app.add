application client-app {
  generator java {
    enabled := true
    javadoc := true

    //{{{ type *

    type * {
      package := "org.autocode.test"
    }

    //}}}
    //{{{ type Client

    @import "Client.add"
    type Client {
      operation := { constructor }
  
      field * { access := public }

      field order {
        operation += { init }
	operation init { access := private }
        type list {
          implementation := "ArrayList"
          implementation "ArrayList" {
	    import := "java.util.ArrayList"
          }
        }
      }
      
      field * {
        operation := { get, set }
      }
    }

    //}}}
    //{{{ type Order

    @import "Order.add"
    type Order {
      operation := { constructor }
      field * {
	operation := { get, set }
      }
    }

    //}}}
    //{{{ type Article

    @import "Article.add"

    //}}}
  }
}
