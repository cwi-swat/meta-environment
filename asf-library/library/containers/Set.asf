equations

  [dup] {X*1, X, X*2, X, X*3} = {X*1, X, X*2, X*3}

  [concat] {X*1} + {X*2} = {X*1,X*2}

  [el] elem(X, {X*1, X, X*2}) = true
  [default-el] elem(X, Set) = false

  [com-1] elem(X, Set) == true
          ====>
          common({X,X*1},Set) = {X} + common({X*1},Set)
  [com-2] elem(X, Set) == false
          ====>
          common({X,X*1},Set) = common({X*1},Set)
  [com-3] common({},Set) = {}
