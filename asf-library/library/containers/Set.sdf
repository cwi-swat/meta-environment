module containers/Set[X]

imports basic/Whitespace

exports
  context-free syntax
    "{" { X ","}* "}"      -> Set[[X]]
    Set[[X]] "+" Set[[X]]  -> Set[[X]] {left}

    elem(X, Set[[X]])      -> Boolean



hiddens
  imports basic/Booleans

  sorts X Set[[X]]

  variables
    "X"[0-9]*  -> X
    "X*"[0-9]* -> {X ","}*


