module languages/sdf2/pretty/Labels

imports languages/sdf2/syntax/Labels
imports languages/sdf2/pretty/Kernel

hiddens
context-free syntax
  "[" Symbol "]" -> Box    {to-box}   
  "[" Label "]"  -> Box    {to-box}   
  "<" Box ">"    -> Symbol {from-box} 

variables
  "Symbol" [0-9]* -> Symbol  
  "Label" [0-9]*  -> Label   
