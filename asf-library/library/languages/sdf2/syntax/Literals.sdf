module sdf2/syntax/Literals
imports sdf2/syntax/Kernel aterm/Literals
exports
  sorts UQLiteral
  lexical syntax
    [a-z]                          -> UQLiteral
    [a-z][A-Za-z0-9\-]*[A-Za-z0-9] -> UQLiteral

  sorts Literal
  context-free syntax
    QuotedLiteral -> Literal
    UQLiteral     -> Literal
    Literal       -> Symbol

  context-free syntax
    Literal "(" {Symbol ","}* ")" "->" Symbol Attributes -> Production {avoid}
