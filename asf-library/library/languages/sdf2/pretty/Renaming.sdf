module languages/sdf2/pretty/Renaming

imports languages/sdf2/syntax/Renaming
imports languages/sdf2/pretty/Kernel

hiddens

context-free syntax

"[" Renamings "]" -> Box {to-box}
"[" Renaming* "]" -> Box* {to-box}
"[" Symbol "]"    -> Box {to-box}
"[" Production "]" -> Box {to-box}

"<" Box ">" -> Renamings {from-box}

variables
"Renaming*" -> Renaming*
"Symbol"[0-9]* -> Symbol
"Production"[0-9]* -> Production