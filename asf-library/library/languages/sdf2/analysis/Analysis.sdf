module languages/sdf2/analysis/Analysis

imports languages/sdf2/analysis/RightNullables
imports languages/sdf2/analysis/Nullables

exports
  context-free syntax
    "analysis" "[[" Grammar "]]" -> Grammar

hiddens
  context-free start-symbols
    Grammar

  lexical syntax
    [A-Za-z]+ -> Symbol
  variables
    "Grammar"[0-9]* -> Grammar
    "Production*"[0-9]* -> Production*
    "Symbol*"[0-9]* -> Symbol*
    "Property"[0-9]* -> Property
    "Property*"[0-9]* -> Property*
