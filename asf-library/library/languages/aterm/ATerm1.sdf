module aterm/ATerm1[Term]

imports basic/Strings

hiddens 
  aliases
   {Term ","}* -> Terms

exports

context-free syntax
  
"ATwarning"(Term) -> Term {built-in("ATermWarning")}
"ATerror"(Term)   -> Term {built-in("ATermError")}
  
"ATreadFromNamedFile"(String) -> Term  {built-in("ATermReadFromNamedFile")}
"ATwriteToNamedTextFile"(Term,String) -> Term {built-in("ATermWriteToNamedTextFile")}
"ATwriteToNamedBinaryFile"(Term,String) -> Term {built-in("ATermWriteToBinaryTextFile")}
