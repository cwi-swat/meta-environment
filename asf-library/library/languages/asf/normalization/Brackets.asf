equations

[remove-brackets-rule]
	length(Symbol*1) == length(Tree*1)
	===>
	brackets(appl(prod([Symbol*1,Symbol,_Symbol*2],Symbol,
			attrs([_Attr*1,bracket,_Attr*2])),
		[Tree*1,Tree,_Tree*2])) = Tree

