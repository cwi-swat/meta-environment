imports Booleans Naturals Identifiers
exports
  sorts IO-VAL VAL
  context-free syntax
     BOOL             -> IO-VAL
     NAT              -> IO-VAL
     IO-VAL           -> VAL
     ID               -> VAL

  variables
     IO-val[1-9]*         -> IO-VAL
     IO-vals[1-9']*       -> {IO-VAL "," }*
     Val[1-9]*            -> VAL
     Vals[1-9']*          -> {VAL "," }*
