   imports Layout

   exports
      sorts LEX-GEN-VAR GEN-VAR
      lexical syntax
         b [0-9]        -> LEX-GEN-VAR
         b [1-9] [0-9]+ -> LEX-GEN-VAR
      context-free syntax
         LEX-GEN-VAR          -> GEN-VAR
         next "(" GEN-VAR ")" -> GEN-VAR

   hiddens
      variables
         m      -> CHAR*
	 k [12] -> CHAR+

