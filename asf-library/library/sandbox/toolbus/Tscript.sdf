module Tscript

imports ProcessDef ToolDef 

exports
  sorts Tscript TscriptDef

  context-free syntax
    ToolDef                             -> TscriptDef {cons("tooldefScript")}
    ProcessDef                          -> TscriptDef {cons("procdefScript")}
    "toolbus" "(" ProcessInvocation ")" -> TscriptDef

  context-free syntax
    TscriptDef*                         -> Tscript {cons("tscript")}

