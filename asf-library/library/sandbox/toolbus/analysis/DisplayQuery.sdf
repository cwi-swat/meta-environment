module DisplayQuery
 
imports ListOperations

exports
  context-free syntax
    evaluate-query(RSCRIPT, String, RSTORE) -> RVALUE
    display-result(RVALUE) -> List

exports
  context-free syntax
    str2rvar(String) -> RVAR

hiddens
  variables
    "$Char*"[0-9]*   -> CHAR*
    "$Rstore"[0-9]*  -> RSTORE
    "$Rvalue"[0-9]*  -> RVALUE
    "$Script"[0-9]*  -> RSCRIPT
    "$String"[0-9]*  -> String
