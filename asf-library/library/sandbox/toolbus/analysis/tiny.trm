process Editor1 is
let
  Editor : term,
  Aap : term,
  Noot : term,
  Pid : int
in
  snd-msg(open(Editor,Pid?)) .
  create(EditorManager,Pid?) .
  create(EditorCreator,Pid?) .
  create(SE, Pid?)
endlet

process Editor2 is
let
  Aap : term,
  Editor : str,
  Pid : int
in
  rec-msg(open(Editor,Pid?)) .
  create(EditorManager,Pid?) .
  create(EditorCreator,Pid?) .
  create(SE, Pid?)
endlet

toolbus(Editor1)

