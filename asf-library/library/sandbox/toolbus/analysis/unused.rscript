rel[<node,str>,node] DECL_OCCURS
rel[<node,str>,node] USE_OCCURS
rel[<node,str>,node] DEF_OCCURS
rel[node,str] DECLS
rel[node,str] USES
rel[node,str] DEFS
bag[str] USED_VARS = range(USES) union range(DEFS)
bag[str] DECLS_VARS = range(DECLS)
bag[str] UNUSED_SIMPLE_VARS = DECLS_VARS \ USED_VARS
rel[node,str] UNUSED_VARS = {<N2,V> | <N1,N2> in DECL_OCCURS, V in UNUSED_SIMPLE_VARS, V == sec(N1)}