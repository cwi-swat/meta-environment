[display-query1] 
  display-query($ActionName, $Script, $String2, $Rstore) =
      convertl($ActionName,
               rval2rel(eval-rscript($Script, str2rvar($String2), $Rstore)))

[display-query2] 
  display-query($ActionName, $Rvalue) = 
    convertl($ActionName, rval2rel($Rvalue))

[sv1] str2rvar(strcon("""  $Char* """)) = rvar($Char*)