process Editor1 is
let
  Editor: term,
  Pid: int
in
  snd-msg(open(Editor, Pid?))
  .
  (
    (
      rec-msg(bla)
    ||
      rec-msg(bla2)
    )
    *
    delta
  )
  .
  (
    create(EditorManager, Pid?)
    . create(EditorCreator, Pid?)
    . create(SE, Pid?)
  )
endlet

process Editor2 is
let
  Editor: str,
  Pid: int
in
  rec-msg(open(Editor, Pid?))
  . create(EditorManager, Pid?)
  . create(EditorCreator, Pid?)
  . create(SE, Pid?)
endlet

toolbus(Editor1, Editor2)
