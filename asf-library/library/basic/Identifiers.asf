equations

[done]
	idcon2strcon(idcon(head)) = strcon(\" strchar(head) \")

[loop]
	strcon(\" char* \") := idcon2strcon(idcon(head2 tail))
	===>
	idcon2strcon(idcon(head1 head2 tail)) =
	strcon(\" strchar(head1) char* \")

equations

[done]
	strcon2idcon(strcon(\" strchar(head) \")) = idcon(head)

[loop]
	idcon(head2 tail) := strcon2idcon(strcon(\" char* \"))
	===>
	strcon2idcon(strcon(\" strchar(head1) char* \")) =
	idcon(head1 head2 tail)
	


