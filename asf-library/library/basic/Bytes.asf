[stb]	string-to-bytes(strcon("""char*""")) = 
		chars-to-bytes(strcon(char*))

[ctb1]	chars-to-bytes(strcon()) = bytes()


[ctb2]	bytes(char*') = chars-to-bytes(strcon(char*))
	===
	chars-to-bytes(strcon("\" "n" char*)) = bytes("
" char*')

[ctb3]	bytes(char*') = chars-to-bytes(strcon(char*))
	===
	chars-to-bytes(strcon("\" "t" char*)) = bytes("	" char*')


[ctb4]	bytes(char*') = chars-to-bytes(strcon(char*))
	===
	chars-to-bytes(strcon("\" """ char*)) = bytes(""" char*')

[ctb5]	bytes(char*') = chars-to-bytes(strcon(char*))
	===
	chars-to-bytes(strcon("\" "\" char*)) = bytes("\" char*')

[default-ctb]
	n = natcon(char1 char2 char3),
	byte(char) = octal-to-byte(n),
	bytes(char*') = chars-to-bytes(strcon(char*))
	===
	chars-to-bytes(strcon("\" char1 char2 char3 char*)) =	
		bytes(char char*')

[default-ctb]
	strcon(char) != strcon("\"),
	bytes(char*') = chars-to-bytes(strcon(char*))
	===
	chars-to-bytes(strcon(char char*)) = bytes(char char*')

[bts]	strcon(char*) = bytes-to-chars(bytes)
	===
	bytes-to-string(bytes) = strcon("""char*""")

[btc1]	bytes-to-chars(bytes()) = strcon()

[btc2]	strcon(char*') = bytes-to-chars(bytes(char*))
	===
	bytes-to-chars(bytes("
" char*)) = strcon("\" "n" char*')

[btc3]	strcon(char*') = bytes-to-chars(bytes(char*))
	===
	bytes-to-chars(bytes("	"char*)) = strcon("\" "t" char*')

[btc4]	strcon(char*') = bytes-to-chars(bytes(char*))
	===
	bytes-to-chars(bytes("\" char*)) = strcon("\" "\" char*')

[btc5]	strcon(char*') = bytes-to-chars(bytes(char*))
	===
	bytes-to-chars(bytes(""" char*)) = strcon("\" """ char*')

[default-btc]
	n = byte-to-octal(byte(char)),
	n >= 40 & n <= 176 = false,
	strcon(char1 char2 char3) = octal-to-string(n),
	strcon(char*') = bytes-to-chars(bytes(char*))
	===
	bytes-to-chars(bytes(char char*)) = 
		strcon("\" char1 char2 char3 char*')

[default-btc]
	n = byte-to-octal(byte(char)),
	n >= 40 & n <= 176 = true,
	strcon(char*') = bytes-to-chars(bytes(char*))
	===
	bytes-to-chars(bytes(char char*)) = strcon(char char*')

[nob1]	num-of-bytes(bytes()) = 0

[nob2]	num-of-bytes(bytes(char char*)) = 1 + num-of-bytes(bytes(char*))

[acmp1]	ascii-compare(bytes, bytes) = 0

[acmp2]	ascii-compare(bytes(char char*1),bytes(char char*2)) =
		ascii-compare(bytes(char*1),bytes(char*2))

[acmp3]	byte(char1) != byte(char2),
	byte-to-octal(byte(char1)) < byte-to-octal(byte(char2)) = true
	===
	ascii-compare(bytes(char1 char*1),bytes(char2 char*2)) = -1

[acmp3]	byte(char1) != byte(char2),
	byte-to-octal(byte(char1)) > byte-to-octal(byte(char2)) = true
	===
	ascii-compare(bytes(char1 char*1),bytes(char2 char*2)) = 1

[default-acmp4]	
	ascii-compare(bytes(), bytes(char+)) = -1

[default-acmp5]	
	ascii-compare(bytes(char+), bytes()) = 1




[otc1]	octal-to-string(natcon(char1)) = strcon("0" "0" char1)

