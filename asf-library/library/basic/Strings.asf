equations
     
[concat]   
strcon(\" c*1 \") || strcon(\" c*2 \") = strcon(\" c*1 c*2 \")

equations

[forward-length]	
length(Str) = num-of-bytes(string-to-bytes(Str))

[forward-less]
Str1 < Str2 = string-to-bytes(Str1) <string-to-bytes(Str2)

[forward-less-equal]
Str1 <= Str2 = string-to-bytes(Str1) <= string-to-bytes(Str2)

[forward-greater-equal]
Str1 >= Str2 = string-to-bytes(Str1) >= string-to-bytes(Str2)

[forward-greater]
Str1 > Str2 = string-to-bytes(Str1) > string-to-bytes(Str2)
