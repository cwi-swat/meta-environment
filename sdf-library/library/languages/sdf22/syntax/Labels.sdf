module languages/sdf22/syntax/Labels

imports languages/sdf22/syntax/Kernel 
        basic/StrCon

exports 
  sorts Label

  context-free syntax
    StrCon -> Label {cons("quoted")}
    IdCon  -> Label {cons("unquoted")}

    Label ":" Sym -> Sym  {cons("label")}
