#include <sglr.idef>
#include <sdf2-language.tb>

process Test is
  snd-msg(open-sdf2-language).
  (
    rec-msg(sdf2-language-opened)
  +
    rec-msg(sdf2-language-not-opened) .
    printf("open-sdf2-lang-test FAILED!")
  +
    shutdown("open-sdf2-lang-test FAILED! (timeout)\n") delay(sec(5))
  ) .
  shutdown("") delay(sec(2))

toolbus(Test, Open-Sdf2-Language, Parser)
