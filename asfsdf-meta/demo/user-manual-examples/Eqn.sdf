module Eqn

imports Layout

exports
  sorts E 

  context-free syntax
    E "sub" E         -> E {left}
    E "sup" E         -> E {left}
    E "sub" E "sup" E -> E 
    "{" E "}"         -> E {bracket}
    "a"               -> E 

  context-free priorities
    E "sub" E "sup" E -> E > 
    {left: E "sub" E -> E 
           E "sup" E -> E}
