module Bubble-Gen

list[&E] sort(list[&E] Elements, bool GreaterThan(&E, &E)){
  for(int I : [1 .. size(Elements) - 1 ]){
     if(GreaterThan(Elements[I],  Elements[I+1])){
       <Elements[I], Elements[I+1]> = <Elements[I+1], Elements[I]>;
       return sort(Elements);
     }
  }
  return Elements;
}