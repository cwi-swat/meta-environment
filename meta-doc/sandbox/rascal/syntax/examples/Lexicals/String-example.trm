module String-example

fun String removeFirstNL(String S){
    switch S {
      case string("\"" <NQChar* Chars1> "\n" <NQChar* Chars2> "\"") =>
           string("\"" <Chars1> <Chars2> "\"")
    }
}

fun String removeAllNL(String S){
    visit S {
      case string("\"" <NQChar* Chars1> "\n" <NQChar* Chars2> "\"") =>
           string("\"" <Chars1> <Chars2> "\"")
    }
}