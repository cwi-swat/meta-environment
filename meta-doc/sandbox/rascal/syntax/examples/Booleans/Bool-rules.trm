module Bool-rules

data Bool btrue;
data Bool bfalse;
data Bool band(Bool L, Bool R);
data Bool bor(Bool L, Bool R);  

arules(Bool) {
      band(btrue, <Bool B2>)  => <B2> 
      band(bfalse, <Bool B2>) => bfalse

      bor(btrue, btrue)       => btrue
      bor(btrue, bfalse)      => btrue
      bor(bfalse, btrue)      => btrue
      bor(bfalse, bfalse)     => bfalse
}