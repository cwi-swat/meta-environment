module DeclAnnos

imports Names

exports
  sorts DeclAnnoChar DeclAnno DeclAnnos

  lexical syntax
    "@" Name "{" DeclAnnoChar* "}"               -> DeclAnno
    ~[\}]                                        -> DeclAnnoChar
    "\\}"                                        -> DeclAnnoChar

  context-free syntax
    DeclAnno*                                    -> DeclAnnos

