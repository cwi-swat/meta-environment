module ProcessDef
imports ProcessExpr

exports
  sorts ProcessDef ProcessDefs

  context-free syntax
   "process" ProcessName "is" ProcessExpr 
				-> ProcessDef  {cons("ProcessDefinition0")}
   "process" ProcessName "(" {VarDecl ","}* ")" "is" ProcessExpr 
				-> ProcessDef  {cons("ProcessDefinition")}
    ProcessDef* 		-> ProcessDefs