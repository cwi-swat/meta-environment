process GUI is
let
  ClassPath: str,
  URLs: list,
  JarName: str,
  ClassName: str
in
  (
    rec-msg(load-jar(JarName?))
    . printf("1: load-jar(%s)", JarName)
  +
    rec-msg(load-jar(JarName?, ClassPath?))
    . printf("2: load-jar2(%s, %s)", JarName, ClassPath)
  +
    rec-msg(load-jar(JarName?, URLs?))
    . printf("3: load-jar(%s, %t)", JarName, URLs)
    )
endlet

process A is snd-msg(load-jar("abc", ["def"]))

toolbus(GUI, A)
