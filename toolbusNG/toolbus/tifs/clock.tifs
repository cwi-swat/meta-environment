tifs(
  version(1),
  package("nl.cwi.toolbus"),
  tool("clock"),
  [
    process(
      name("Clock"),
      [
        eval(get-time-of-day, Time)
      ]
    ),
    process(
      name("Timer"),
      [
        eval(start-timing, int),
        eval(elapsed(int), Time),
        eval(stop-timing(int), Time)
      ]
    ),
    process(
      name("Alarm"),
      [
        do(set-alarm(Time,String)),
        event(alarm(String))
      ]
    )
  ]
)
