tifs(
  version(1),
  package("nl.cwi.toolbus"),
  tool("clock"),
  [
    process(
      name("Clock"),
      [
        eval(timeofday, Time),
        eval(start-timing, int),
        eval(elapsed(int), Time),
        eval(stop-timing(int), Time),
        do(set-alarm(Time,String)),
        event(alarm(String))
      ]
    )
  ]
)
