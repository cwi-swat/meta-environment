
tool example1 is { kind = "java" command="toolbus.tool.java.Example1" }

process EXAMPLE1 is
let
  T : example1,
  Count : int,
  Name  : str
in
  execute(example1, T?) .
  (
    (  
      snd-eval(T, msg("Hello World!")) .
      rec-value(T, count(Count?)) .
      printf("Hello number %d\n", Count)
    )  * delta 
    ||
    ( rec-event(T, button(Name?)) .
      printf("Button pressed: %s\n", Name) .
      snd-ack-event(T, button(Name))
    ) * delta
  )
endlet

toolbus(EXAMPLE1)

