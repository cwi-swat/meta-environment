module ExtendedIntegers

imports basic/Integers

exports
   context-free syntax
	Integer "/" Integer	-> Integer
	Natural "/" Natural	-> Natural
	max(Integer, Integer)	-> Integer
	min(Integer, Integer)	-> Integer

hiddens
   imports asf/syntax/Comments

   variables	
    "$Nat" [0-9\']*      -> Natural 
    "$Nat-con" [0-9\']*  -> NatCon
    "$Int" [0-9\']*      -> Integer
