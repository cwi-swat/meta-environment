begin declare x : natural, y : natural, 
              z : natural, p : natural;
  x := 3;
  p := 4;
  if q then 
        z := y + x
  else
        x := 4
  fi;
  y := z
end

,

type stat = area
type var = str
type var-occ = area

stat ROOT

rel[stat,stat] PRED
rel[stat,var] USES
rel[stat,var] DEFS
rel[var,var-occ] OCCURS

rel[stat, var] UNINIT =
   { <S, V>| <stat S, var V> : USES, <ROOT, S> in reachX({ROOT}, DEFS[-,V], PRED)
   }

rel[var, var-occ] UNINIT-OCC =
   { <V, VO> | <stat S, var V> : UNINIT, var-occ VO : OCCURS[V], VO <= S }


,

