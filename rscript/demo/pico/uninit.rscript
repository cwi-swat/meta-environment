
 	area ROOT
        rel[area,area] PRED
        rel[<area,str>,area] USE_OCCURS
        rel[<area,str>,area] DEF_OCCURS
        rel[area,str] USES
        rel[area,str] DEFS

        rel[str,area] occurrences(<area,str> S) = {second(S)} x USE_OCCURS[S]

        rel[str,area] UNINIT = 
        { occurrences(<N,V>) | <N, V> : USES, <ROOT, N> in reachX({ROOT}, DEFS . V, PRED)
        }

                         