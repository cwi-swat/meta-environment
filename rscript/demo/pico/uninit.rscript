		   type expr = loc
		   type var  = loc
		   type name = str

		   expr ROOT

		   rel[expr,expr] PRED
		   rel[expr,name] DEFS
		   rel[var,name] USES
		   bag[name] VARNAMES  = range(USES)


		   rel[name, var] UNINIT =
		   { <N, V>| name N : VARNAMES, 
                             var V : USES[-,N],
			     <ROOT, expr E> : reachX({ROOT}, DEFS[-,N], PRED),
			     V <= E
		}

