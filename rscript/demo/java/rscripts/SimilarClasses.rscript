
rel[str,str] CALL
rel[str,str] CONTAINMENT
rel[str,str] INHERITANCE

rel[str,str] ALLREL = CALL union CONTAINMENT union INHERITANCE

bag[str] CLASSES = domain(ALLREL)

rel[str,str] IDENT = {<X, Y> | X : CLASSES, Y : CLASSES, X < Y,
                                CALL[X] == CALL[Y],
				CONTAINMENT[X] == CONTAINMENT[Y],
				INHERITANCE[X] == INHERITANCE[Y]
}

int size = # IDENT

yield size

yield IDENT






