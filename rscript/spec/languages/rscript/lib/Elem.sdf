module languages/rscript/lib/Elem

imports
	basic/NatCon
	basic/BoolCon
	basic/StrCon
	languages/rscript/lib/Nested-Relations[Elem]

hiddens
  context-free start-symbols
	Elem

exports

sorts Elem

  context-free syntax
	NatCon -> Elem
	BoolCon -> Elem
	StrCon -> Elem

  context-free syntax
	<Elem, Elem, {Elem ","}+> -> Elem

hiddens
  imports basic/Whitespace

  variables
	"elem"[0-9\']* -> Elem
	"elem*"[0-9\']* -> {Elem ","}*
	"elem+"[0-9\']* -> {Elem ","}+

