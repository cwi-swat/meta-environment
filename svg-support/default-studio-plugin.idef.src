tool default-studio-plugin is { }

process DefaultStudioPlugin is
let
  T : default-studio-plugin
in
  rec-connect(T?)
  .
  (
    <interface>
  )
  * delta
endlet

process StartDefaultStudioPlugin is
  snd-msg(load-jar("file://__PKGDATA__/__JARFILE__", "__LOCAL_JARS__:__EXTERNAL_JARS__"))

toolbus(DefaultStudioPlugin)
