%%    $ProcessExpr1 + $ProcessExpr3 + $ProcessExpr2

[choice-normalization-0]
  $ProcessExpr1 . ( $ProcessExpr2 + $ProcessExpr3 ) = 
    ( $ProcessExpr1 . $ProcessExpr2 + $ProcessExpr1 . $ProcessExpr3 )

[choice-normalization-1]
  ( $ProcessExpr1 + $ProcessExpr2 ) . $ProcessExpr3 =
    ( $ProcessExpr1 . $ProcessExpr3 + $ProcessExpr2 . $ProcessExpr3 )

[choice-normalization-2]
  $ProcessExpr + $ProcessExpr = $ProcessExpr

[comm-0]
   $ProcessExpr1 + ($ProcessExpr2  + $ProcessExpr3) =
    ($ProcessExpr1 +  $ProcessExpr2) + $ProcessExpr3 

[comm-1]
   $ProcessExpr1 . ($ProcessExpr2  . $ProcessExpr3) =
    ($ProcessExpr1 .  $ProcessExpr2) . $ProcessExpr3 

[default-star-normalization]
  $ProcessExpr1 * $ProcessExpr2 = $ProcessExpr1 . $ProcessExpr2 + $ProcessExpr2

[star-normalization-0]
  ( $ProcessExpr1 + $ProcessExpr2 ) * $ProcessExpr3 =
    ( $ProcessExpr1 ) * $ProcessExpr3 + $ProcessExpr2 . $ProcessExpr3 + $ProcessExpr3

[if-then-0]
  if $Term then $ProcessExpr1 fi . $ProcessExpr2 =
    if $Term then $ProcessExpr1 . $ProcessExpr2 fi

[if-then-1]
  $ProcessExpr1 . if $Term then $ProcessExpr2 fi . $ProcessExpr3 =
    $ProcessExpr1 . if $Term then $ProcessExpr2 . $ProcessExpr3 fi

[if-then-else-0]
  if $Term then $ProcessExpr1 else $ProcessExpr2 fi . $ProcessExpr3 =
    if $Term then $ProcessExpr1 . $ProcessExpr3 else $ProcessExpr2 . $ProcessExpr3 fi

[if-then-else-1]
  $ProcessExpr1 . if $Term then $ProcessExpr2 else $ProcessExpr3 fi . $ProcessExpr4 =
    $ProcessExpr1 . if $Term then $ProcessExpr2 . $ProcessExpr4 else $ProcessExpr3 . $ProcessExpr4 fi

