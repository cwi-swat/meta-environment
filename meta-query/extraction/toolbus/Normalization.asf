equations

%%[idem-0]
%%  $ProcessExpr1 + $ProcessExpr1 = $ProcessExpr1

%%[idem-1]
%%  $ProcessExpr1 + $ProcessExpr2 + $ProcessExpr2 = $ProcessExpr1 + $ProcessExpr2

%%[idem-2]
