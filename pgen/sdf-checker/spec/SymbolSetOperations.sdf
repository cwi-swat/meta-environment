module SymbolSetOperations

imports sdf2/syntax/Sdf2 basic/Booleans

exports 
  sorts SymbolsSet

  context-free syntax
    symbols(Symbol*)             -> SymbolsSet
    diff(SymbolsSet, SymbolsSet) -> SymbolsSet
    contains(Symbol, SymbolsSet) -> Boolean

hiddens
  variables
    "$Symbol*"[0-9]*    -> Symbol*
    "$Symbol"[0-9]*     -> Symbol
    "$Symbols"[0-9]*    -> Symbols
