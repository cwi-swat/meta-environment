module SymbolSetOperations

imports Sdf2 Booleans

exports 
  sorts SymbolsSet

  context-free syntax
    symbols(Symbol*)             -> SymbolsSet
    diff(SymbolsSet, SymbolsSet) -> SymbolsSet
    contains(Symbol, SymbolsSet) -> Bool

hiddens
  variables
    "$Symbol*"[0-9]*    -> Symbol*
    "$Symbol"[0-9]*     -> Symbol
    "$Symbols"[0-9]*    -> Symbols