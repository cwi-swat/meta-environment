equations

  [cs-1'1] check-for-deprecated-symbols(Definition) = 
             check-for-deprecated-symbols(Definition, {})

  [cs-1'2] check-for-deprecated-symbols(Module, Msgs) = 
             check-for-deprecated-symbols2(Module, Msgs, 
                                           get-module-name(Module))

  [cl-2'1] check-for-deprecated-symbols2($Symbol1 # $Symbol2, Msgs, $ModuleId) =
             Msgs + deprecated-symbol($ModuleId, $Symbol1 # $Symbol2)
