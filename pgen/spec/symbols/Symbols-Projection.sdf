module Symbols-Projection
imports Symbols Booleans Integers
exports
  context-free syntax
    Symbols "++" Symbols -> Symbols {assoc}
    "|" Symbols "|"      -> Int
    Symbol "(-" Symbols  -> Bool

hiddens
  variables
    "_"[ABC][0-9\']*    -> Symbol
    "'"[abc][0-9\']*    -> Symbol*
    "'"[abc]"+"[0-9\']* -> Symbol+
