# @configure_input@
# directory: tbviz/spec/tbviz

PRODUCT 	= @PRODUCT@
VERSION 	= @VERSION@

SHELL		= /bin/sh
srcdir		= @srcdir@
VPATH		= @srcdir@
top_srcdir	= @top_srcdir@

CLEAN           = @CLEAN@
INSTALL		= @INSTALL@

@SET_MAKE@
prefix 		= @prefix@
exec_prefix 	= @exec_prefix@
bindir 		= $(exec_prefix)/bin
specdir         = $(exec_prefix)/spec/tbviz

SPECFILES       = TBviz.eqs TBviz.syn GraphEnv.eqs MatchTable.eqs \
                  PrettyPrint.eqs GraphEnv.syn MatchTable.syn PrettyPrint.syn

TARGETFILES     = ${SPECFILES} ctasdf.conf   dump-it.ll
DISTFILES	= Makefile.in ${TARGETFILES}

all:
clean:
	${CLEAN}
distclean:
	clean
	rm -f Makefile
install:
	for file in ${TARGETFILES}; do \
	  $(INSTALL) -m 644 $$file in $(specdir); \
	done

uninstall:
	for file in ${TARGETFILES}; do \
	  rm -f $(specdir)/$$file; \
	done
