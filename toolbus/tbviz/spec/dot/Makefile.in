# @configure_input@
# directory: tbviz/spec/dot

PRODUCT 	= @PRODUCT@
VERSION 	= @VERSION@

SHELL		= /bin/sh
srcdir		= @srcdir@
VPATH		= @srcdir@
top_srcdir	= @top_srcdir@

CLEAN           = @CLEAN@
INSTALL		= @INSTALL@

@SET_MAKE@
prefix 		= @prefix@
exec_prefix 	= @exec_prefix@
bindir 		= $(exec_prefix)/bin
specdir         = $(exec_prefix)/spec/dot

SPECFILES       = Booleans2.eqs Dot-syntax.eqs Naturals-operations.syn \
                  Booleans2.syn Dot-syntax.syn Naturals-syntax.eqs \
                  Dot-operations.eqs Naturals-syntax.syn Dot-operations.syn \
                  Naturals-operations.eqs

TARGETFILES     = ${SPECFILES}
DISTFILES	= Makefile.in ${TARGETFILES}

all:
clean:
	${CLEAN}
distclean:
	clean
	rm -f Makefile
install:
	for file in ${SPECFILES}; do \
	  $(INSTALL) -m 644 $$file in $(specdir); \
	done

uninstall:
	for file in ${SPECFILES}; do \
	  rm -f $(specdir)/$$file; \
	done
