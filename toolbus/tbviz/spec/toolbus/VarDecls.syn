%%  VarDecls

imports Terms

exports
  sorts VarDecl VarDecls
  lexical syntax
    <LexicalFunction+>
  context-free syntax
    VAR ":" TERM                -> VarDecl
    VAR ":" TERM "?"            -> VarDecl
    { VarDecl "," }*            -> VarDecls
  variables
    VDecl[0-9']*        -> VarDecl
    VDecls[0-9']*       -> { VarDecl "," }*
