# @configure_input@
# directory: tbviz

PRODUCT 	= @PRODUCT@
VERSION 	= @VERSION@

SHELL		= /bin/sh

@SET_MAKE@

prefix 		= @prefix@
exec_prefix 	= @exec_prefix@
bindir		= @bindir@
INSTALL         = @INSTALL@
top_srcdir      = @top_srcdir@

PROGRAMS        = tbgraph informer uri-encode

all clean distclean dist:

install:
	for f in $(PROGRAMS); \
	do \
		$(INSTALL) -m 755 $$f $(bindir); \
	done

uninstall:
	for f in $$PROGRAMS; \
	do \
		rm -f $(bindir)/$f ; \
	done
