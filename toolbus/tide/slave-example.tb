
%%
%% slave-example.tb: An example of a ToolBus slave system.
%%
%% The main purpose of this example is to generate a meaningful viewer.tif.c,
%% and to do some initial testing.
%%

process FUBAR is
let
  Count : int
in
  printf("FUBAR started!\n") .
  Count := 1 .
  ( printf("FUBAR loop %d\n", Count) .
    Count := add(Count,1)
  ) * delta
endlet

tool viewer is { command="./buscontrol" }

toolbus(FUBAR)