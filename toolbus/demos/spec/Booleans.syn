   exports
      lexical syntax
         [ \t\n]          -> LAYOUT
         "%%" ~[\n]* "\n" -> LAYOUT

   exports
      sorts BOOL
      context-free syntax
         "true"          -> BOOL
         "false"         -> BOOL
         BOOL "|" BOOL -> BOOL {left}
         BOOL "&" BOOL -> BOOL {left}
         "not" "(" BOOL ")" -> BOOL
         "(" BOOL ")"  -> BOOL {bracket}
      variables
         Bool [0-9']* -> BOOL

   priorities
      "|" < "&"

