# @configure_input@
# demos/perl

PRODUCT		= @PRODUCT@
VERSION		= @VERSION@

srcdir		= @srcdir@
VPATH		= @srcdir@
top_srcdir	= @top_srcdir@
prefix		= @prefix@
exec_prefix	= @exec_prefix@
bindir		= @bindir@
libdir		= @libdir@
includedir	= $(prefix)/include

SHELL           = /bin/sh
CLEAN           = @CLEAN@

.SUFFIXES:
.SUFFIXES: .c .o

PROGRAMS	=
DISTFILES	= Makefile.in test.tb

all:	${PROGRAMS}

clean:
	${CLEAN}

distclean:	clean
	rm -f Makefile
 
dist: $(DISTFILES)
	@for file in $(DISTFILES); do \
	  ln $(srcdir)/$$file $(top_srcdir)/../$(PRODUCT)-$(VERSION)/demos/perl 2> /dev/null \
	  || cp $(srcdir)/$$file $(top_srcdir)/../$(PRODUCT)-$(VERSION)/demos/perl; \
	done
