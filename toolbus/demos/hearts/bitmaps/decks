# The deck id (for instance goyette) must be the same as the subdirectory
# in which the cards are located.
set decks(default) "traub"

# The deck 'Goyette'
lappend deckList goyette
set decks(goyette,name) "Goyette"
set decks(goyette,width) 49
set decks(goyette,height) 66

# The deck 'Large'
lappend deckList large
set decks(large,name) "Large"
set decks(large,width) 84
set decks(large,height) 123

# The deck 'Sager'
lappend deckList sager
set decks(sager,name) "Sager"
set decks(sager,width) 55
set decks(sager,height) 69

# The deck "b/w"
lappend deckList bw
set decks(bw,name) "b/w"
set decks(bw,width) 64
set decks(bw,height) 93

# The deck 'Traub'
lappend deckList traub
set decks(traub,name) "Traub"
set decks(traub,width) 76
set decks(traub,height) 99


#### End of decks. The following table defines the translation
#### between card numbers and bitmap files.
set decks(table,2) "spades.2"
set decks(table,3) "spades.3"
set decks(table,4) "spades.4"
set decks(table,5) "spades.5"
set decks(table,6) "spades.6"
set decks(table,7) "spades.7"
set decks(table,8) "spades.8"
set decks(table,9) "spades.9"
set decks(table,10) "spades.10"
set decks(table,11) "spades.jack"
set decks(table,12) "spades.queen"
set decks(table,13) "spades.king"
set decks(table,14) "spades.ace"

set decks(table,18) "hearts.2"
set decks(table,19) "hearts.3"
set decks(table,20) "hearts.4"
set decks(table,21) "hearts.5"
set decks(table,22) "hearts.6"
set decks(table,23) "hearts.7"
set decks(table,24) "hearts.8"
set decks(table,25) "hearts.9"
set decks(table,26) "hearts.10"
set decks(table,27) "hearts.jack"
set decks(table,28) "hearts.queen"
set decks(table,29) "hearts.king"
set decks(table,30) "hearts.ace"

set decks(table,34) "diamonds.2"
set decks(table,35) "diamonds.3"
set decks(table,36) "diamonds.4"
set decks(table,37) "diamonds.5"
set decks(table,38) "diamonds.6"
set decks(table,39) "diamonds.7"
set decks(table,40) "diamonds.8"
set decks(table,41) "diamonds.9"
set decks(table,42) "diamonds.10"
set decks(table,43) "diamonds.jack"
set decks(table,44) "diamonds.queen"
set decks(table,45) "diamonds.king"
set decks(table,46) "diamonds.ace"

set decks(table,50) "clubs.2"
set decks(table,51) "clubs.3"
set decks(table,52) "clubs.4"
set decks(table,53) "clubs.5"
set decks(table,54) "clubs.6"
set decks(table,55) "clubs.7"
set decks(table,56) "clubs.8"
set decks(table,57) "clubs.9"
set decks(table,58) "clubs.10"
set decks(table,59) "clubs.jack"
set decks(table,60) "clubs.queen"
