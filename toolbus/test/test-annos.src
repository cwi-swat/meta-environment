top_builddir=__TOP_BUILDDIR__
srcdir=__SRCDIR__

# This test sometimes hangs, we give it a reasonable cpu limit
ulimit -t 25

TERM="f(a{[b,c]})"
(echo "$TERM" | ${top_builddir}/toolbus/toolbus ${srcdir}/anno.tb) > test.out 2>&1
fgrep "result:$TERM,$TERM" test.out > /dev/null 2>&1
if [ $? -ne 0 ]; then
  exit 1
fi
 
TERM="f(a{[b,c]}){[an,no]}"
(echo "$TERM" | ${top_builddir}/toolbus/toolbus ${srcdir}/anno.tb) > test.out 2>&1
fgrep "result:$TERM,$TERM" test.out > /dev/null 2>&1
if [ $? -ne 0 ]; then
  rm -f test.out
  exit 1
fi


