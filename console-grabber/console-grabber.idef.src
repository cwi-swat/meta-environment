tool console-grabber is { }

process ConsoleGrabber is
let
  T : console-grabber,
  Message: str
in
  rec-connect(T?)
  .
  (
    rec-event(T, message(Message?))
    . snd-note(cg-console-message(Message))
  )
  * rec-disconnect(T)
endlet

toolbus(ConsoleGrabber)
