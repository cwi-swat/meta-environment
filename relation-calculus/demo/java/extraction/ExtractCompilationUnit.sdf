module ExtractCompilationUnit

imports Rstore
imports JavaOperations
imports utilities/PosInfo[CompilationUnit]

exports
  context-free syntax
    extractCompilationUnit(CompilationUnit, Rel[[Elem]]) -> Rel[[Elem]]

hiddens
  variables
    "&Loc"		-> Location
    "&CompilationUnit"	-> CompilationUnit
    "&Rel"[0-9]*	-> Rel[[Elem]]
