module muasf2nametable/MuASF2NameTable

imports utils/VarEnv


exports

context-free syntax

muasf2nametable(Module) -> MN-struct

hiddens

context-free syntax
  rename-signatureopt(SignatureOpt, NameTable) -> SignatureOpt
  rename-rulesopt(RulesOpt, NameTable)     -> RulesOpt
  rename-funcdeflist(FuncDefList, NameTable)  -> FuncDefList
  rename-funcdef(FuncDef, NameTable)      -> FuncDef
  rename-termlist(TermList, NameTable)     -> TermList
  rename-funid(FunId, NameTable)        -> FunId
  rename-cond(Cond,NameTable) -> Cond
  rename-condlist(CondList,NameTable)  -> CondList
  rename-rule(Rule,NameTable) -> Rule
  rename-rulelist(RuleList,NameTable) -> RuleList
  rename-term(Term,NameTable) -> Term

  "rename-term" -> FunId {reject}
  "rename-funid" -> FunId {reject}
  

context-free syntax

  create-name-table(SignatureOpt,NameTable) -> NameTable

