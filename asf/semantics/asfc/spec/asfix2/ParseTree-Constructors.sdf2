%% Introduce function symbols for each elementary notion
%% that may occur in a parse tree.
module ParseTree-Constructors

imports ATerms
exports
  context-free syntax
    "w"             -> AFun
    "l"             -> AFun
    "sort"          -> AFun
    "meta-var"      -> AFun
    "sep"           -> AFun
    "iter"          -> AFun
    "iter-sep"      -> AFun
