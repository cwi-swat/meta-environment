/* $Id$ */

process FilterEqsSyntax is
let
  FES : filtereqssyntaxtool,
  Tree : term,
  Filtered : term,
  Ambs : term
in
  (
    rec-msg(filtereqssyntax(Tree?)).
    execute(filtereqssyntaxtool, FES?).
    snd-eval(FES, filtereqssyntax(Tree)).
    rec-value(FES,filtereqssyntax(Filtered?,Ambs?)).
    snd-msg(filtereqssyntax(Tree, Filtered, Ambs)).
    snd-terminate(FES,"done")
  ) *
  delta
endlet

tool filtereqssyntaxtool is { command = "filtereqssyntaxtool" }

   

